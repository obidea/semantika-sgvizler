<?xml version="1.0" encoding="utf-8"?>
<program xmlns="http://www.obidea.com/ns/termal#"
   xmlns:tml="http://www.obidea.com/ns/termal#"
   xmlns:rr="http://www.w3.org/ns/r2rml#">
   <prefix tml:name="" tml:ns="http://data.worldbank.org/data-catalog/indodapoer#" />
   <prefix tml:name="wbld" tml:ns="http://worldbank.270a.info/" />
	
   <uri-template tml:name="AgriFunction" tml:value="http://worldbank.270a.info/FC.XPD.AGR.CR/{1}#{2}" />
   <uri-template tml:name="BPKAudit" tml:value="http://worldbank.270a.info/BPK.AUD.SUBN/{1}#{2}" />

   <mapping tml:id="Agriculture Function">
      <logical-table rr:tableName="statistics" />
      <subject-map rr:class="wbld:AgricultureFunction" rr:template="AgriFunction(Region_Name_Code, Time_Value_Code)"/>
      <predicate-object-map rr:predicate="value" rr:column="FC.XPD.AGR.CR"/>
      <predicate-object-map rr:predicate="year" rr:column="Time_Value"/>
      <predicate-object-map rr:predicate="dataOrigin" rr:column="Region_Name"/>
      <predicate-object-map rr:predicate="dataLevel" rr:column="Region_Type"/>
   </mapping>
   <mapping tml:id="BPK Audit on Sub-National Budget">
      <logical-table rr:tableName="statistics" />
      <subject-map rr:class="wbld:BPKAudit_SubNationalBudget" rr:template="BPKAudit(Region_Name_Code, Time_Value_Code)"/>
      <predicate-object-map rr:predicate="value" rr:column="BPK.AUD.SUBN"/>
      <predicate-object-map rr:predicate="year" rr:column="Time_Value"/>
      <predicate-object-map rr:predicate="dataOrigin" rr:column="Region_Name"/>
      <predicate-object-map rr:predicate="dataLevel" rr:column="Region_Type"/>
   </mapping>
</program>